  "description": "Galactic off-chain routing & optimization of orders across pools for best price execution",
  "author": "GalacticCouncil",
  "repository": {
    "directory": "packages/sdk",
    "type": "git",
    "url": "git+https://github.com/galacticcouncil/sdk.git"
  },
  "keywords": [
    "hydration",
    "basilisk",
    "router",
    "sdk"
  ],
